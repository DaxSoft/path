{
    "name": "@vorlefan/path",
    "version": "1.8.34",
    "description": "Easy access and handler for folders and files paths with several tools",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "author": {
        "name": "Michael Willian Santos",
        "email": "dax-soft@live.com",
        "url": "https://path.vorlefan.com/"
    },
    "keywords": [
        "path",
        "routes",
        "files",
        "folder",
        "tree"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
        "prepublish": "yarn run tsc",
        "format": "prettier \"**/*\" --ignore-unknown",
        "format:write": "yarn run format --write",
        "lint:prune": "ts-prune",
        "lint:spelling": "cspell \"**\" \".github/**/*\"",
        "prepare": "husky install",
        "test": "vitest"
    },
    "license": "MIT",
    "private": false,
    "devDependencies": {
        "@typescript-eslint/parser": "^5.38.1",
        "@vitest/coverage-istanbul": "^0.25.8",
        "husky": "^8.0.2",
        "lint-staged": "^13.1.0",
        "octokit": "^2.0.10",
        "prettier": "^2.7.1",
        "release-it": "^15.5.1",
        "ts-prune": "^0.10.3",
        "typescript": "^4.9.4",
        "vitest": "^0.26.2"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/DaxSoft/path"
    },
    "dependencies": {}
}
